{
  "name": "react-app",
  "version": "0.0.1",
  "description": "New App",
  "main": "index.tsx",
  "author": "Dmitry Melnikov",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --info=false --mode development --config ./config/webpack.dev.js",
    "start:verbose": "webpack-dev-server --mode development",
    "start:dist": "yarn clean && yarn build && lite-server -c ./config/serve_dist.js",
    "clean": "rm -rf dist && mkdir dist && cp -R public/* dist",
    "wipe": "rm -rf dist && rm -rf docs && rm -rf node_modules",
    "build": "yarn clean && webpack --mode production --config ./config/webpack.dev.js",
    "docs:build": "./node_modules/typedoc/bin/typedoc",
    "docs:serve": "lite-server -c ./config/serve_docs.js",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx src"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.1",
    "typescript": "^3.7.2"
  },
